[application]

config/name="Hop Spin Dance"
run/main_scene="res://emptyscene.tscn"
run/low_processor_mode=true
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

ConnectionUtil="*res://addons/gd_util/connection_util.gd"
Console="*res://addons/quentincaffeino-console/src/Console.tscn"
GodotMathExtension="*res://addons/math_util/math_funcs.gd"
LogManager="*res://addons/sar1_log_manager/log_manager.gd"
ModelFormat="*res://addons/model_format/model_format.gd"
NetworkLogger="*res://addons/network_manager/network_logger.gd"
NetworkManager="*res://addons/network_manager/network_manager.gd"
InputManager="*res://addons/input_manager/input_manager.gd"
EntityManager="*res://addons/entity_manager/entity_manager.gd"

[display]

window/size/width=1600
window/size/height=900
window/size/resizable=false
window/dpi/allow_hidpi=true
window/stretch/mode="canvas_items"
window/stretch/aspect="keep"
window/vsync/vsync_via_compositor=true

[editor]

import/use_multiple_threads=false

[editor_plugins]

enabled=PackedStringArray("res://addons/Godot-MToon-Shader/plugin.cfg", "res://addons/entity_manager/plugin.cfg", "res://addons/gd_util/plugin.cfg", "res://addons/input_manager/plugin.cfg", "res://addons/math_util/plugin.cfg", "res://addons/mesh_combiner/plugin.cfg", "res://addons/model_format/plugin.cfg", "res://addons/network_manager/plugin.cfg", "res://addons/quentincaffeino-console/plugin.cfg", "res://addons/sar1_log_manager/plugin.cfg", "res://addons/static_baker/plugin.cfg", "res://addons/vrm/plugin.cfg", "res://addons/vsk_avatar/plugin.cfg", "res://addons/vsk_vrm_avatar_tool/plugin.cfg")

[entities]

config/process_priority=0

[importer_defaults]

scene={
"meshes/create_shadow_meshes": false
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[network]

config/networked_scenes=PackedStringArray()
config/max_networked_entities=4096
config/network_fps=60
config/entity_root_node=NodePath("")
config/compression_mode=0
config/default_port=7777

[rendering]

textures/vram_compression/import_bptc=true
threads/thread_model=2
vram_compression/import_etc2=false
quality/screen_filters/msaa=2
quality/screen_filters/screen_space_aa=1
quality/screen_filters/use_debanding=true
environment/default_environment="res://vrm/thirdparty/uiux/stage_env.tres"
quality/skinning/software_skinning_fallback=false
quality/filters/msaa=3
